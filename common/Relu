class Relu:
    def __init__(self):
        self.mask = None
        
    def forward(self, x):
 #       self.mask = zeros(length(x))
 #       self.mask(x<=0) = 0 
        
        out = x[self.mask]
        
        return out
        
    def backward(self, dout):
#        dx = zeros(length(dout))
#        dx = ones(dout(dout>0))
        dx = ones(dout(length)) 
        dx[self.mask] = 0
        return dx
